    "chai": "^4.2.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.2.0",
    "mochapack": "^2.0.3",
    "moxios": "^0.4.0",
    "vue-loader": "^15.9.3",
